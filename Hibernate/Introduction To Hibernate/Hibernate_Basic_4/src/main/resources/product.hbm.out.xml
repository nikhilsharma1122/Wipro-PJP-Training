<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.wipro.emp.Product" table = "hr.product">
		<id name="ProductId" column="ProductId" type="int">
			<generator class = "sequence"></generator>
		</id>
		<property name="ProductName" column="ProductName" type="string"></property>
		<property name="Price" column = "Price" type="int"></property>
		
	</class>
</hibernate-mapping>
