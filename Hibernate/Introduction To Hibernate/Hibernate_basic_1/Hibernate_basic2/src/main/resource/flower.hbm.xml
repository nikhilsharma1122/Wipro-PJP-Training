<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.wipro.bean.Flower" table="hr.flower">
		<id name="price" column = "price" type="int">
			<generator class="assigned"></generator>
		</id>
		<property name = "FlowerID" column = "FlowerID" type="string"></property>
		<property name = "FlowerName" column = "FlowerName" type="string"></property>
		<property name = "Color" column = "Color" type="string"></property>
		
	</class>
</hibernate-mapping>
